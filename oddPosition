// print odd postiions of the string

class Main{
    public static void main(String[] args){
        String name = "SAIGUTTULASAI";
        for(int i=0;i<name.length();i++){
            if(i%2 == 0){
                charAt(i).toUpperCase();
            }
        }
        System.out.println(name);
    }
}
