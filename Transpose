class Main{
    public static void main(String[] args){
        int[][] arr={{1,2,3},{4,5,6},{7,8,9}};
        int sum=0;
        int n=arr.length;
        int[][] arr2= new int[n][n];
        for(int i=0;i<n;i++){
            for(int j=0;j<arr[i].length;j++){
           arr2[j][i] = arr[i][j];
        }
    }  
        for(int i=0;i<n;i++){
            for(int j=0;j<arr[i].length;j++){
           System.out.print(arr2[i][j]+ " ");
        }
        System.out.println();
        }
    }
}
