class Factorial{
    public static void main(String[] args) {
        int num = 3;
        long factorial = 1;
        
        if(num == 0) {
            System.out.println("1");
        } else {
        
        for(int i = 1; i <= num; i++) {
            factorial *= i;
        }    
            System.out.println(factorial);
        }
    }
}
