
class Main{
    public static void main(String[] args) {
        String str = "SAIIAS";
        boolean isPalindrome = true;
        
        int start = 0;
        int end = str.length() -1;
        
        if(str.charAt(start) != str.charAt(end)) {
            isPalindrome = false;
        }
        start++;
        end--;
        
        System.out.println(isPalindrome);
    }
}
