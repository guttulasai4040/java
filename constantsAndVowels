class constantsAndVowels {
    public static void main(String[] args) {
        String str = "Sai The Bear";
        int vowels = 0;
        int constants = 0;
        for(char c : str.toCharArray()) {
            if("aeiouAEIOU".indexOf(c) != -1) {
                vowels++;
            } else  if (Character.isLetter(c)) {
                constants++;                
            }
        }
        
        System.out.println("No of: " + vowels + " and" + " " + constants);
    }
}
