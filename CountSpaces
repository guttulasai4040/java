class CountSpaces{ 
    public static void main(String[] args) {
    String letter = "Hey         I am sai guttula";
    int count = 0;
    
    for(int i = 0; i < letter.length() - 1; i++) {
        if(letter.charAt(i) == ' ') {
            count++;
        }
    }
    
    System.out.println("this is numbers of the letters: " + count);
    }
}
