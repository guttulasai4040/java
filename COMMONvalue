class Main{
    public static void main(String[] args){
        int[] arr = {1,2,3,4,5};
        int[] arr1 = {2,3,4,5,6};
        int Value = 0;
        
       for(int i=0;i<arr.length;i++) {
          for(int j=0;j<arr1.length;j++){
              if(arr[i] == arr1[j]){
                  Value = arr[i];
                  break;
              }
          }
       }
       System.out.println(Value);
    }
}
