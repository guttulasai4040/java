class SwapNumbers {
    public static void main(String[] args) {
        int a = 10;
        int b = 20;
        
        // Swap numbers using a temporary variable
        int temp = a;
        a = b;
        b = temp;
        
        // Output the result after swapping
        System.out.println("After swapping: a = " + a + ", b = " + b);
    }
}
