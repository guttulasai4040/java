class Main{
    public static void main(String[] args){
        int n=5;
        int result = fact(n);
        
        System.out.println(result);
    }
    
    public static int fact(int a){
        if(a == 0 || a == 1){
            return 1;
        } else {
            return a*fact(a-1);
        }
    }
}
