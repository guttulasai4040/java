class palidromeNum {
    public static void main(String[] args) {
        int  num = 121;
        int reverse = 0; 
        int original = num;
        int rem ;
        
        while(num != 0) {
            rem = num % 10;
            reverse = reverse * 10 + rem;
            num = num / 10;
        }
        
        if (original == reverse) {
            System.out.println("yes"); 
        } else {
            System.out.println("no");
        }
    }
}
